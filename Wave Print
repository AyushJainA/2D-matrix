#include <bits/stdc++.h> 
vector<int> wavePrint(vector<vector<int>> arr, int nRows, int mCols)
{   vector<int> ans;

//approach -- even/odd column 
   for(int i =0 ;i<mCols;i++){
       
       if(i % 2==0 /* even */ ){
           for(int j =0; j<nRows;j++){
               ans.push_back(arr[j][i]);
                }   } 
      else{ for(int k =nRows-1;k>=0;k--){
          ans.push_back(arr[k][i]);
                 }
          }     
   
} return ans; }
